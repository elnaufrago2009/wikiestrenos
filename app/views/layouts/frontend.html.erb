
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>WikiEstrenos - Peliculas de estreno gratis online</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Le styles -->
    <%= stylesheet_link_tag "wikiestrenos-front-end" %>
    <%= javascript_include_tag "wikiestrenos-front-end" %>

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

  </head>

  <body>

    <!-- Menu front end -->
    <%= render "shared/menu_frondend" %>
    <!-- End Menu front end -->

    <div class="container" id="contenido">
      <%= yield %>
      <footer>
        <hr>
        <p>Estrenos 2013</p>
      </footer>

    </div> <!-- /container -->

    <!-- Configuracion popover -->
    <script>
        $("a[data-toggle=popover]")
                .popover({ html : true })
                .click(function(e) {
                    e.preventDefault()
                })
    </script>
    <!-- End Configuracion popover -->

  </body>
</html>
